@model IEnumerable<WebTutorial.Models.ComplicatedModel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.str1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.str2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.str3)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.str4)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.str5)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.str6)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str4)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str5)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.str6)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
